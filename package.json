    "@next/bundle-analyzer": "15.1.4",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "autoprefixer": "10.4.20",
    "encoding": "0.1.13",
    "eslint": "9.17.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "0.6.9",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2"
  },
  "peerDependencies": {
    "typescript": "5.7.2"
  },
  "packageManager": "pnpm@9.15.3"
