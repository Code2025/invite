    "postcss": "8.4.33",
    "prettier": "3.2.1",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.14.1"
