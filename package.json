  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squarestack/github-invite/issues"
  },
  "homepage": "https://just-that.vercel.app",
  "dependencies": {
    "@octokit/rest": "20.0.2",
    "@vercel/analytics": "1.1.1",
    "geist": "1.2.1",
    "next": "14.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "1.3.1"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "2.2.0",
    "@igorkowalczyk/is-browser": "5.0.2",
